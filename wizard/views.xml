<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_inventory_menu" model="ir.ui.view">
            <field name="name">inventory.adjustment.wizard.form</field>
            <field name="model">product.inv</field>
            <field name="arch" type="xml">
                <form string="Inventory Movement">
                    <group>
                        <field name="product"/>
                        <field name="start_date"/>
                        <field name="end_date"/>
                        <field name="location"/>
                    </group>
                    <footer>
                        <button name="%(inventory_report)d" type="action" string="Print Report"/>
                    </footer>
                </form>
            </field>
        </record>

</odoo>
